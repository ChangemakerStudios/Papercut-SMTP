{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },

  "SmtpServer": {
    "IP": "127.0.0.1"
  },

  "AllowedHosts": "*",

  "Urls": "http://localhost:37408"
}